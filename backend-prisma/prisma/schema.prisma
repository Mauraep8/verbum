generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model verbs {
  id             Int     @id
  verb           String  @db.Text
  verb_group     String  @db.Text
  bescherelle_id Int
  primary_verb   String  @db.Text
  special_type   String? @db.Text
  auxiliary      String? @db.Text
}
